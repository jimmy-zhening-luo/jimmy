    //
    // #region Editor
    // "disableSizeLimit": false,
    // "plugins": [],
    // #endregion
    //
    // #region Interop
    "allowSyntheticDefaultImports": true,
    // "erasableSyntaxOnly": true,
    // "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
