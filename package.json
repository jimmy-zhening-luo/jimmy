{
  "$pkg": "2211.6.0",
  "$schema": "https://json.schemastore.org/package",
  "$help": "https://docs.npmjs.com/cli/configuring-npm/package-json",
  "name": "jimmy",
  "version": "11.2.1",
  "repository": "github:jimmy-zhening-luo/jimmy",
  "description": "This repository contains Jimmy Luo's personal website. As of August 31, 2022, the main branch of this repository is continuously deployed to https://jimm.my.",
  "keywords": [
    "typescript",
    "sveltekit",
    "azure-static-web-apps"
  ],
  "license": "MIT",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "^22",
    "npm": "^10 || ^11"
  },
  "type": "module",
  "devDependencies": {
    "@fontsource/open-sans": "^5.2.7",
    "@fontsource/overpass-mono": "^5.2.8",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.47.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "eslint-plugin-svelte": "~3.12.4",
    "linted": "32.5.0",
    "svelte-check": "^4.3.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.10"
  },
  "scripts": {
    "clean": "cmd --% /c \"(if exist dist rmdir /s /q dist) & (if exist build rmdir /s /q build) & (if exist .eslintcache del /f /q .eslintcache)\" || rm -rf dist build .eslintcache",
    "prebuild": "npm run clean",
    "build": "svelte-kit sync && svelte-check && vite build",
    "postbuild": "npm run lint || echo \"Lint failed. Continuing...\"",
    "lint": "eslint --cache --fix",
    "pretest": "npm run build",
    "test": "npm run --if-present test:runner",
    "prestart": "npm run --ignore-scripts build",
    "start": "vite preview --open",
    "predeploy": "npm test",
    "deploy": ""
  },
  "author": "Jimmy Zhening Luo <jimmy-zhening-luo@users.noreply.github.com> (https://jimm.my/)"
}
