{
  "name": "jimmy",
  "version": "10.0.0-rc.6",
  "description": "This repository contains Jimmy Luo's personal website. As of August 31, 2022, the main branch of this repository is continuously deployed to https://jimm.my.",
  "keywords": [
    "typescript",
    "sveltekit",
    "azure-static-web-apps"
  ],
  "license": "MIT",
  "type": "module",
  "private": true,
  "config": {
    "PACKAGE_JSON_VERSION": "20.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/jimmy.git"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/jimmy/issues"
  },
  "author": {
    "name": "Jimmy Zhening Luo",
    "email": "1450044+jimmy-zhening-luo@users.noreply.github.com",
    "url": "https://jimm.my"
  },
  "main": "",
  "types": "",
  "peerDependenciesMeta": {},
  "dependencies": {
    "@fontsource/open-sans": "^5.0.28",
    "@fontsource/overpass-mono": "^5.0.18",
    "@fontsource/oxygen": "^5.0.13"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "autoprefixer": "^10.4.19",
    "eslint": "~8.57.0",
    "linted": "~14.1.1",
    "postcss": "^8.4.39",
    "svelte-check": "^3.8.4",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ]
  },
  "scripts": {
    "eslint": "eslint --cache --fix",
    "lint:svelte": "(svelte-kit sync) && (svelte-check --tsconfig ./tsconfig.json) && (npm run eslint -- src/**/*.svelte)",
    "lint:source": "npm run eslint -- src/**/*.{js,ts,html}",
    "lint:configs": "npm run eslint -- {*,.github/workflows/*}.{json,jsonc,js,ts,yml}",
    "lint": "(npm run --if-present lint:svelte) && (npm run lint:source) && (npm run lint:configs)",
    "prebuild": "npm run lint",
    "build": "(vite build) || (tsc)",
    "pretest": "npm run build",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" NODE_NO_WARNINGS=1 jest --passWithNoTests --",
    "prepublishOnly": "npm test",
    "prestart": "npm test",
    "start": "(vite preview) || (node .)",
    "preaction": "npm test",
    "action": "exit 0"
  }
}
