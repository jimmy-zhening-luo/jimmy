  "keywords": [
    "typescript",
    "sveltekit",
    "azure-static-web-apps"
  ],
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:jimmy-zhening-luo/jimmy",
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "bugs": "https://github.com/jimmy-zhening-luo/jimmy/issues",
  "type": "module",
  "main": "",
  "types": "",
  "engineStrict": true,
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.8.2"
  },
  "peerDependenciesMeta": {},
  "dependencies": {
    "@fontsource/open-sans": "^5.0.30",
    "@fontsource/overpass-mono": "^5.0.20",
    "@fontsource/oxygen": "^5.0.15"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.26",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "~9.10.0",
    "linted": "~16.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "svelte-check": "^4.0.1",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.3"
  },
  "scripts": {
    "build": "run-s \"build:{1}:*\" lint:built  --  svelte",
    "build:ts:compile": "tsc",
    "build:svelte:sync": "svelte-kit sync",
    "build:svelte:check": "svelte-check --tsconfig ./tsconfig.json",
    "build:svelte:compile": "vite build",
    "build:svelte:prelint": "npm run ln src/**/*.svelte",
    "lint": "npm run build",
    "lint:built": "run-s lint:built:*",
    "lint:built:configs": "npm run ln --files={*,.github/workflows/*}.{json,jsonc,js,ts,yml}",
    "lint:built:source": "npm run ln --files=src/**/*.{js,ts,html}",
    "ln": "eslint --cache --fix --no-error-on-unmatched-pattern %npm_config_files% || echo \"No linter installed.\"",
    "pretest": "npm run build",
    "test": "",
    "start": "vite preview || node .",
    "prestart": "npm test",
    "prepublishOnly": "npm test",
    "predeploy": "npm test",
    "deploy": ""
  },
  "author": "Jimmy Zhening Luo <1450044+jimmy-zhening-luo@users.noreply.github.com> (https://jimm.my/)"
