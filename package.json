{
  "name": "jimmy",
  "description": "Jimmy Luo personal website",
  "version": "8.1.2",
  "author": "Zhening Luo",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/jimmy.git"
  },
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/jimmy/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "scripts": {
    "clean": "rimraf --glob ./dist/*",
    "tsc": "tsc",
    "copy-assets": "ts-node tools/copyAssets",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "postbuild": "npm run copy-assets",
    "start": "node ./dist/index.js",
    "predev:start": "npm run build",
    "dev:start": "npm run start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start"
  },
  "engines": {
    "npm": ">=10.2.0",
    "node": ">=18.18.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "node": "^18.18.0",
    "npm": "^10.2.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.3",
    "@types/express": "^4.17.19",
    "@types/express-session": "^1.17.8",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^18.18.0",
    "@types/shelljs": "^0.8.13",
    "fs-extra": "^11.1.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}