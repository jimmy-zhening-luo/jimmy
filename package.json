    "azure-static-web-apps"
  ],
  "license": "MIT",
  "type": "module",
  "private": true,
  "config": {
    "PACKAGE_JSON_VERSION": "20.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/jimmy.git"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/jimmy/issues"
  },
  "author": {
    "name": "Jimmy Zhening Luo",
    "email": "1450044+jimmy-zhening-luo@users.noreply.github.com",
    "url": "https://jimm.my"
  },
  "main": "",
  "types": "",
  "peerDependenciesMeta": {},
  "dependencies": {
    "@fontsource/open-sans": "^5.0.28",
    "@fontsource/overpass-mono": "^5.0.18",
    "@fontsource/oxygen": "^5.0.13"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "autoprefixer": "^10.4.19",
    "eslint": "~8.57.0",
    "linted": "~14.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "svelte-check": "^3.8.4",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ]
  },
  "scripts": {
    "build": "run-s \"build:{1}:*\" lint:built  --  svelte",
    "build:ts:compile": "tsc",
    "build:svelte:sync": "svelte-kit sync",
    "build:svelte:check": "svelte-check --tsconfig ./tsconfig.json",
    "build:svelte:compile": "vite build",
    "build:svelte:prelint": "npm run ln src/**/*.svelte",
    "lint": "npm run build",
    "lint:built": "run-s lint:built:*",
    "lint:built:configs": "npm run ln {*,.github/workflows/*}.{json,jsonc,js,ts,yml}",
    "lint:built:source": "npm run ln src/**/*.{js,ts,html}",
    "ln": "eslint --cache --fix",
    "pretest": "npm run build",
    "test": "",
    "start": "vite preview || node .",
    "prestart": "npm test",
    "prepublishOnly": "npm test",
    "predeploy": "npm test",
    "deploy": ""
  }
}
