{
  "name": "jimmy",
  "version": "9.10.10",
  "description": "Jimmy Luo personal website",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/jimmy.git"
  },
  "author": "Jimmy Zhening Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/jimmy/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "scripts": {
    "lint": "eslint --cache --cache-strategy content -c eslint.config.js svelte.config.js vite.config.ts src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "predev": "npm run lint:fix",
    "dev": "vite dev",
    "prebuild": "npm run lint:fix",
    "build": "vite build",
    "postbuild": "npm run check",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm run check -- --watch",
    "prepreview": "npm run build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@jimbojet/lint": "^6.1.4",
    "@stylistic/eslint-plugin": "^1.8.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.38.0",
    "svelte": "^4.2.15",
    "svelte-check": "^3.7.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.0.28",
    "@fontsource/overpass-mono": "^5.0.18",
    "@fontsource/oxygen": "^5.0.13"
  }
}
