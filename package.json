  },
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "scripts": {
    "es": "eslint --fix --cache --cache-strategy content -c eslint.config.js",
    "lint": "npm run lint:config && npm run lint:src",
    "lint:config": "npm run lint:config:js && npm run lint:config:ts",
    "lint:src": "npm run lint:src:ts && npm run lint:src:svelte",
    "lint:config:js": "npm run es -- *.config.js",
    "lint:config:ts": "npm run es -- *.config.ts",
    "lint:src:ts": "npm run es -- src/**/*.ts",
    "lint:src:svelte": "npm run es -- src/**/*.svelte",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm run check -- --watch",
    "prebuild": "npm run check && npm run lint",
    "build": "vite build",
    "prepreview": "npm run build",
    "preview": "vite preview",
    "test": "npm run preview",
    "dev": "vite dev"
  },
  "devDependencies": {
    "@jimbojet/lint": "~9.1.3",
    "@stylistic/eslint-plugin": "~2.1.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "eslint": "~8.57.0",
    "eslint-plugin-svelte": "~2.39.0",
    "svelte": "^4.2.17",
    "svelte-check": "^3.8.0",
    "svelte-eslint-parser": "~0.36.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.0.28",
    "@fontsource/overpass-mono": "^5.0.18",
    "@fontsource/oxygen": "^5.0.13"
  }
}
