{
  "$pkg": "2411.4.1",
  "$schema": "https://json.schemastore.org/package",
  "$help": "https://docs.npmjs.com/cli/configuring-npm/package-json",
  "name": "jimmy",
  "version": "11.4.3",
  "repository": "github:jimmy-zhening-luo/jimmy",
  "description": "This repository contains Jimmy Luo's personal website. As of August 31, 2022, the main branch of this repository is continuously deployed to https://jimm.my.",
  "keywords": [
    "typescript",
    "sveltekit",
    "azure-static-web-apps"
  ],
  "license": "MIT",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "^24",
    "npm": "^11"
  },
  "type": "module",
  "devDependencies": {
    "@fontsource/open-sans": "^5.2.7",
    "@fontsource/overpass-mono": "^5.2.8",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "eslint-plugin-svelte": "~3.13.0",
    "linted": "33.4.2",
    "svelte-check": "^4.3.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "scripts": {
    "clean": "cmd --% /c \"for %G in (types dist dist-tests build packed) do (if exist %G rmdir /s /q %G) & for %G in (.eslintcache) do (if exist %G del /f /q %G)\" || rm -rf types dist dist-tests build packed .eslintcache",
    "build": "npm run build:svelte",
    "build:svelte": "svelte-kit sync && svelte-check && vite build",
    "postbuild": "npm run lint || exit 0",
    "lint": "eslint --cache --fix",
    "pretest": "npm run build",
    "test": "npm run --if-present test:suite",
    "prestart": "npm run build",
    "start": "npm run start:svelte",
    "start:svelte": "vite preview --open",
    "predeploy": "npm run clean && npm test",
    "stage": "",
    "deploy": ""
  },
  "author": "Jimmy Zhening Luo <jimmy-zhening-luo@users.noreply.github.com> (https://jimm.my/)"
}
