{
  "name": "jimmy",
  "version": "9.13.41",
  "description": "This repository contains Jimmy Luo's personal website. As of August 31, 2022, the main branch of this repository is continuously deployed to https://jimm.my.",
  "keywords": [
    "typescript",
    "sveltekit",
    "azure-static-web-apps"
  ],
  "type": "module",
  "private": false,
  "main": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/jimmy.git"
  },
  "author": "Jimmy Zhening Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/jimmy/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "scripts": {
    "es": "eslint --fix --cache --cache-strategy content -c eslint.config.js",
    "lint": "npm run lint:cfg && npm run lint:src && npm run lint:ci",
    "lint:cfg": "npm run lint:cfg:pkg && npm run lint:cfg:tsc && npm run lint:cfg:linter && npm run lint:cfg:svelte && npm run lint:cfg:vite",
    "lint:cfg:pkg": "npm run es -- package.json",
    "lint:cfg:tsc": "npm run es -- tsconfig.json",
    "lint:cfg:linter": "npm run es -- eslint.config.js",
    "lint:cfg:svelte": "npm run es -- svelte.config.js",
    "lint:cfg:vite": "npm run es -- vite.config.ts",
    "lint:src": "npm run lint:src:ts && npm run lint:src:svelte && npm run lint:src:html",
    "lint:src:ts": "npm run es -- src/**/*.ts",
    "lint:src:svelte": "npm run es -- src/**/*.svelte",
    "lint:src:html": "npm run es -- src/**/*.html",
    "lint:ci": "npm run lint:ci:github",
    "lint:ci:github": "npm run es -- .github/workflows/*.yml",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm run check -- --watch",
    "prebuild": "npm run check && npm run lint",
    "build": "vite build",
    "prepreview": "npm run build",
    "preview": "vite preview",
    "test": "npm run preview",
    "dev": "vite dev"
  },
  "devDependencies": {
    "@jimbojet/lint": "~10.8.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "eslint": "~8.57.0",
    "svelte-check": "^3.8.0",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.13"
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.0.28",
    "@fontsource/overpass-mono": "^5.0.18",
    "@fontsource/oxygen": "^5.0.13"
  }
}
