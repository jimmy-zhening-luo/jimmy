{
  "name": "jimmy",
  "description": "Jimmy Luo personal website",
  "version": "8.0.0",
  "author": "Zhening Luo",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/jimmy.git"
  },
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/jimmy/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/jimmy#readme",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "tsc": "tsc",
    "copy-assets": "ts-node tools/copyAssets",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "postbuild": "npm run copy-assets",
    "start": "node ./dist/index.js",
    "predev:start": "npm run build",
    "dev:start": "npm run start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start"
  },
  "engines": {
    "npm": ">=8.11.0",
    "node": ">=16.16.0"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "node": "^16.16.0",
    "npm": "^8.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.56",
    "@types/shelljs": "^0.8.11",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
