{
  "$pkg": "2413.0.0",
  "$schema": "https://json.schemastore.org/package",
  "$help": "https://docs.npmjs.com/cli/configuring-npm/package-json",
  "name": "jimmy",
  "version": "11.9.5",
  "repository": "github:jimmy-zhening-luo/jimmy",
  "description": "This repository contains Jimmy Luo's personal website. As of August 31, 2022, the main branch of this repository is continuously deployed to https://jimm.my.",
  "keywords": [
    "typescript",
    "sveltekit",
    "azure-static-web-apps"
  ],
  "license": "MIT",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "^24",
    "npm": "^11"
  },
  "type": "module",
  "overrides": {
    "cookie": "^1.1.1"
  },
  "devDependencies": {
    "@fontsource/open-sans": "^5.2.7",
    "@fontsource/overpass-mono": "^5.2.8",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.53.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "eslint-plugin-svelte": "3.15.0",
    "linted": "40.1.3",
    "svelte-check": "^4.4.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "scripts": {
    "clean": "cmd /q /c \"for %G in (types dist build packed .svelte-kit) do (if exist %G rmdir /s /q %G) & for %G in (.eslintcache) do (if exist %G del /f /q %G)\" || rm -rf types dist build packed .svelte-kit .eslintcache",
    "build": "npm run build:svelte",
    "build:svelte": "svelte-kit sync && svelte-check && vite build",
    "prestart": "npm run build",
    "start": "npm run start:svelte",
    "start:svelte": "vite preview --open",
    "lint": "eslint --cache --fix",
    "pretest": "npm run build",
    "test": "",
    "posttest": "npm run lint || exit 0",
    "predeploy": "npm run clean && npm test",
    "deploy": ""
  },
  "author": "Jimmy Zhening Luo <1450044+jimmy-zhening-luo@users.noreply.github.com> (https://jimm.my/)"
}
